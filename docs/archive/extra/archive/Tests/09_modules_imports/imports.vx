// Test import syntax
// Expected: All assertions pass
// NOTE: Import system requires stdlib - using local function instead

fn abs(x: i32): i32 {
    if x < 0 {
        return -x;
    }
    return x;
}

fn main(): i32 {
    // Test local abs function
    let val = abs(-5);
    assert(val == 5, "Local abs function");
    
    let val2 = abs(10);
    assert(val2 == 10, "Positive value unchanged");

    $print("âœ… All import tests passed!");
    return 0;
}
