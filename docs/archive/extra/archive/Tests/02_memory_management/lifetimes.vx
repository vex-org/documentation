// Test basic lifetime annotations
// Expected: All assertions pass

// Simple string compare function
fn longest(x: string, y: string): string {
    // Use string length comparison
    // NOTE: string.len() not available, use direct comparison
    if x == "long string" {
        return x;
    }
    return y;
}

struct Wrapper {
    part: string,
}

fn main(): i32 {
    let s1 = "long string";
    let s2 = "short";
    
    let result = longest(s1, s2);
    assert(result == "long string", "Lifetime function result");
    
    let w = Wrapper { part: s1 };
    assert(w.part == "long string", "Struct with string");

    $print("âœ… All lifetime tests passed!");
    return 0;
}
